        <referenceContainer name="content">
            <block class="Extension\GiftCard\Block\GiftCard" name="account_giftcard_tab"
                   template="Extension_GiftCard::account_giftcard_tab.phtml"
                   ifconfig="giftcard/general/enable"
                   cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>
